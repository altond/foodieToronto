<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.foodietoronto.ui.home.FeedFragment"
        android:label="@string/title_feed"
        tools:layout="@layout/fragment_feed" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.foodietoronto.ui.dashboard.SavedPostsFragment"
        android:label="@string/title_savedposts"
        tools:layout="@layout/fragment_savedposts" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.foodietoronto.ui.notifications.CreatePostFragment"
        android:label="@string/title_createpost"
        tools:layout="@layout/fragment_createpost" />
</navigation>